{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-moz-backface-visibility",
          "text-align: -webkit-match-parent",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "disown-opener": "off"
  }
}